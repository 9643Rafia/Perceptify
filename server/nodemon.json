{
  "watch": [
    "controllers",
    "models",
    "routes",
    "middleware",
    "server.js",
    "config"
  ],
  "ext": "js,json",
  "ignore": [
    "../node_modules",
    "../build",
    "../public",
    "server_backup",
    "*.test.js"
  ],
  "env": {
    "NODE_ENV": "development",
    "PORT": "5000"
  },
  "delay": "1000"
}